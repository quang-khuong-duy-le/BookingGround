<!-- Docs nav
  ================================================== -->
<div class="container">
    <div class="row">
        <div class="page-header">
            <h1>
                <p style="margin-left: 37%; color: #8E849F;">
                    Danh Sách Đặt Sân</p>
            </h1>
        </div>
        <table class="table">
            <thead>
                <tr>
                    <td>
                        Tên Sân
                    </td>
                    <td>
                        Địa Chỉ
                    </td>
                    <td>
                        Thời Gian Đặt
                    </td>
                    <td>
                        Thời Gian Đá
                    </td>
                    <td>
                        Bắt Đầu Từ
                    </td>
                    <td>
                        Hủy Sân
                    </td>
            </thead>
            <tbody>
                @foreach (BookingGround.Models.tblBookingList list in Model.bookingList)
                {
                    <tr class="success">
                        <td>
                            @list.tblGroundTime.tblChildGround.tblMasterGround.name
                        </td>
                        <td>
                            @list.tblGroundTime.tblChildGround.tblMasterGround.address
                        </td>
                        <td>
                            @list.date
                        </td>
                        <td>
                            @list.tblGroundTime.tblChildGround.tblInterval.interval phút
                        </td>
                        <td>
                            @list.tblGroundTime.tblTimeTable.timestart
                        </td>
                        <td>
                            @Html.ActionLink("Hủy đặt sân", "Cancel", new { id = list.id })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
        <div class="span9">
        </div>
    </div>
</div>
